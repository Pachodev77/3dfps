{
  "git": {
    "deploymentEnabled": {
      "lfs": true
    }
  },
  "functions": {
    "api/server.js": {
      "runtime": "node18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ]
}