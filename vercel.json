{
  "git": {
    "deploymentEnabled": {
      "lfs": true
    }
  },
  "routes": [
    {
      "src": "/socket.io.js",
      "dest": "/socket.io.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}